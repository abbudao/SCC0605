PARSER_BEGIN(Jason)
import java.util.ArrayList;
import java.util.List;
import Simbolo.Simbolo;
    class Jason{
    List<Simbolo> symbol_table = new ArrayList<Simbolo>();
    int nivel_atual = 0;
    int err_count = 0;
    int nparam = 0;
    int arg_count = 0;
    boolean isParam = false;
    public Simbolo findSymbol(String id){
    	for(Simbolo s : symbol_table) {
	        if(s.id.equals(id)) {
	            return s;
	        }
	    }
	    return null;
    }
    public int deleteSymbols(int nivel){
    	for(Simbolo s : symbol_table){
    		if(s.nivel == nivel){
    			symbol_table.remove(symbol_table.indexOf(s));
    		}
    	}
    	return 0;
    }

    public static void main(String[] args)
        throws ParseException, TokenMgrError{
	        Jason parser = new Jason(System.in);
	        parser.Program();
        }

    }
PARSER_END(Jason)

